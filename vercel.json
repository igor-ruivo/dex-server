{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "functions": {
    "api/data.ts": {
      "maxDuration": 30
    },
    "api/trigger.ts": {
      "maxDuration": 60
    },
    "api/proxy.ts": {
      "maxDuration": 30
    },
    "api/status.ts": {
      "maxDuration": 10
    }
  }
}