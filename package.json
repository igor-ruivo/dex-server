{
    "name": "pokemon-data-generator",
    "version": "1.0.0",
    "description": "Simple Pokemon GO data generator with GitHub Actions",
    "main": "generate-data.ts",
    "scripts": {
        "generate": "tsx generate-data.ts",
        "lint": "node scripts/run-linter-wrapper.js && node scripts/run-prettier-wrapper.js && pnpm tsc --noEmit",
        "build": "pnpm tsc",
        "debug": "npx tsx --inspect-brk ./generate-data.ts"
    },
    "keywords": [
        "pokemon",
        "data",
        "github-actions"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@types/jsdom": "^21.1.7",
        "jsdom": "^26.1.0"
    },
    "devDependencies": {
        "@ti-platform/aide-build-tools": "^3.0.0",
        "@tsconfig/recommended": "^1.0.10",
        "@types/node": "^24.0.13",
        "@typescript-eslint/eslint-plugin": "^8.36.0",
        "@typescript-eslint/parser": "^8.36.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-json": "^3.1.0",
        "eslint-plugin-simple-import-sort": "^12.0.0",
        "prettier": "^3.6.2",
        "ts-node": "^10.9.2",
        "tsx": "^4.20.3",
        "typescript": "^5.8.3"
    }
}
